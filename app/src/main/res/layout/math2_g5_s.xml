<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView145"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:paddingTop="25dp"
        android:text="Ви пройшли 27 гру!"
        android:textAlignment="center"
        android:textColor="#000"
        android:textSize="24sp"
        android:textStyle="bold" />

    <ProgressBar
        android:id="@+id/progressBar102"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:max="320"
        android:progress="190" />

    <ImageView
        android:id="@+id/imageView328"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_margin="10dp"
        android:onClick="toMain_math2_g5"
        app:srcCompat="@drawable/home" />

    <ImageView
        android:id="@+id/imageView329"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_margin="10dp"
        android:onClick="go_math2_g6_1"
        app:srcCompat="@drawable/play" />

    <ImageView
        android:id="@+id/imageView428"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignStart="@+id/imageView328"
        android:layout_alignTop="@+id/imageView431"
        app:srcCompat="@drawable/red_circle" />

    <ImageView
        android:id="@+id/imageView430"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignTop="@+id/imageView327"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="90dp"
        app:srcCompat="@drawable/blue_triangle" />

    <ImageView
        android:id="@+id/imageView431"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignStart="@+id/imageView329"
        android:layout_below="@+id/textView145"
        android:layout_marginRight="10dp"
        android:layout_marginTop="110dp"
        app:srcCompat="@drawable/green_square" />

    <ImageView
        android:id="@+id/imageView308"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:paddingBottom="80dp"
        android:paddingTop="150dp"
        app:srcCompat="@drawable/robo_hands" />
</RelativeLayout>