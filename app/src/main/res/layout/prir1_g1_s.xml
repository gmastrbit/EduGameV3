<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView94"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:paddingTop="25dp"
        android:text="Ви пройшли 16 гру!"
        android:textAlignment="center"
        android:textColor="#000"
        android:textSize="24sp"
        android:textStyle="bold" />

    <ProgressBar
        android:id="@+id/progressBar60"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:max="160"
        android:progress="50" />

    <ImageView
        android:id="@+id/imageView219"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_margin="10dp"
        android:onClick="toMain_prir1_g1"
        app:srcCompat="@drawable/home" />

    <ImageView
        android:id="@+id/imageView220"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="10dp"
        android:layout_marginRight="10dp"
        android:onClick="go_prir1_g2_1"
        app:srcCompat="@drawable/play" />

    <ImageView
        android:id="@+id/imageView419"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView94"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="90dp"
        app:srcCompat="@drawable/potatos" />

    <ImageView
        android:id="@+id/imageView421"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_below="@+id/textView94"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="60dp"
        app:srcCompat="@drawable/cucumbers" />

    <ImageView
        android:id="@+id/imageView422"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignEnd="@+id/imageView220"
        android:layout_alignTop="@+id/imageView419"
        app:srcCompat="@drawable/carrots" />

    <ImageView
        android:id="@+id/imageView218"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView419"
        android:layout_centerVertical="true"
        android:paddingTop="80dp"
        app:srcCompat="@drawable/robo_hands" />
</RelativeLayout>