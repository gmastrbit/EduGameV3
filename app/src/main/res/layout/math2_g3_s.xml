<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView134"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:paddingTop="25dp"
        android:text="Ви пройшли 25 гру!"
        android:textAlignment="center"
        android:textColor="#000"
        android:textSize="24sp"
        android:textStyle="bold" />

    <ProgressBar
        android:id="@+id/progressBar94"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:max="320"
        android:progress="110" />

    <ImageView
        android:id="@+id/imageView304"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_margin="10dp"
        android:onClick="toMain_math2_g3"
        app:srcCompat="@drawable/home" />

    <ImageView
        android:id="@+id/imageView305"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_margin="10dp"
        android:onClick="go_math2_g4_1"
        app:srcCompat="@drawable/play" />

    <ImageView
        android:id="@+id/imageView429"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_below="@+id/textView134"
        android:layout_centerHorizontal="true"
        app:srcCompat="@drawable/kulki" />

    <ImageView
        android:id="@+id/imageView296"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:paddingBottom="80dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:paddingTop="150dp"
        app:srcCompat="@drawable/robo_wow_one" />

</RelativeLayout>