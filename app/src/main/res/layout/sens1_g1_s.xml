<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/rel"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:id="@+id/textView29"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:paddingTop="25dp"
        android:text="Ви пройшли 1 гру!"
        android:textAlignment="center"
        android:textColor="#000"
        android:textSize="24sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_margin="10dp"
        android:onClick="toMain_sens1_g1"
        app:srcCompat="@drawable/home" />

    <ImageView
        android:id="@+id/imageView30"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_margin="10dp"
        android:onClick="go_sens1_g2_1"
        app:srcCompat="@drawable/play" />

    <ProgressBar
        android:id="@+id/progressBar3"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:max="410"
        android:progress="50" />

    <ImageView
        android:id="@+id/imageView411"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignStart="@+id/imageView29"
        android:layout_alignTop="@+id/imageView415"
        android:onClick="to_dod1_g1"
        app:srcCompat="@drawable/yellow" />

    <ImageView
        android:id="@+id/imageView415"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignEnd="@+id/imageView30"
        android:layout_alignParentTop="true"
        android:layout_marginTop="165dp"
        android:onClick="to_dod1_g1"
        app:srcCompat="@drawable/green" />

    <ImageView
        android:id="@+id/imageView416"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignStart="@+id/imageView411"
        android:layout_alignTop="@+id/imageView417"
        android:onClick="to_dod1_g1"
        app:srcCompat="@drawable/blue" />

    <ImageView
        android:id="@+id/imageView417"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/imageView415"
        android:layout_marginTop="85dp"
        android:onClick="to_dod1_g1"
        app:srcCompat="@drawable/red" />

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:paddingTop="80dp"
        app:srcCompat="@drawable/robo_hands" />
</RelativeLayout>